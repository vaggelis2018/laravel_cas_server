{
    "name": "leo108/laravel_cas_server",
    "description": "A laravel package provides CAS server implementation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "leo108",
            "email": "leo108@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-dom": "*",
        "illuminate/support": "5.5.*|5.6.*",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/dbal": "^2.6"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
